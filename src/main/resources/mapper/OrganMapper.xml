<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.OrganMapper">
    <insert id="insert" parameterType="OrganDTO">
        insert into injoy."organ" ("organName", "domain")
        values (#{organName}, #{domain})

    </insert>
    <select id="selectLast" resultType="int">
        select "organId" from  injoy."organ"
        order by "organId" DESC  limit 1;
    </select>

    <select id="selectAll" resultType="OrganDTO">
        select "organId" from  injoy."organ"

    </select>


</mapper>
